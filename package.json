    "mocha": "10.2.0",
    "nock": "13.3.1",
    "nyc": "15.1.0",
    "sinon": "15.0.4",
    "ts-loader": "9.4.2",
    "ts-mocha": "10.0.0",
    "typescript": "4.4.4"
